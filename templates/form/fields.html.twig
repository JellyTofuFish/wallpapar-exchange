{% block custom_file_widget %}
    <div class="custom-file">
        {{ form_widget(form.file) }}

        {% if file_uri is defined and file_uri is not null %}
            <div class="col-2">
                <img class="img-thumbnail my-5" width="400" height="400" src="{{ file_uri }}"  alt="">
            </div>
        {% endif %}
    </div>
{% endblock %}